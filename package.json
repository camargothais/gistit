{
  "name": "gistit",
  "version": "1.0.0",
  "description": "Quick and easy code snippet sharing",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fabricio7p/gistit.git"
  },
  "keywords": [
    "cli",
    "tui",
    "snippet",
    "sharing"
  ],
  "author": "fabricio7p <fabricio7p@protonmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fabricio7p/gistit/issues"
  },
  "homepage": "https://github.com/fabricio7p/gistit#readme",
  "devDependencies": {
    "turbo": "^1.0.9"
  },
  "turbo": {
    "baseBranch": "origin/master",
    "pipeline": {
      "build": {
        "dependsOn": [
          "^build"
        ],
        "outputs": [
          ".next/**"
        ]
      },
      "test": {
        "dependsOn": [
          "^build"
        ],
        "outputs": []
      },
      "lint": {
        "outputs": []
      },
      "dev": {
        "cache": false
      }
    }
  }
}
